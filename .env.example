# Private key for deployment (without 0x prefix)
# 64文字の16進数文字列（例: 1234567890abcdef...）
PRIVATE_KEY=your_private_key_here

# Base Sepolia RPC URL (optional, defaults to public RPC)
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Deployed contract address (for minting)
CONTRACT_ADDRESS=

# Recipient address for minting (optional, defaults to deployer)
RECIPIENT_ADDRESS=

# Token URI for NFT metadata
TOKEN_URI=https://example.com/nft/1

# Basescan API key for contract verification (optional)
BASESCAN_API_KEY=